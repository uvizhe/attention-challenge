{
  "build": {
    "beforeBuildCommand": "trunk build --release",
    "beforeDevCommand": "trunk serve",
    "devPath": "http://localhost:8080",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Attention Challenge",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false
    },
    "bundle": {
      "active": true,
      "category": "Lifestyle",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "../icons/96x96.png",
        "../icons/72x72.png",
        "../icons/48x48.png",
        "../icons/32x32.png",
        "../icons/128x128.png",
        "../icons/128x128@2x.png",
        "../icons/icon.icns",
        "../icons/icon.ico"
      ],
      "identifier": "org.supersapiens.achallenge",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 640,
        "resizable": false,
        "title": "Attention Challenge",
        "width": 400
      }
    ]
  }
}
